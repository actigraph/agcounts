@article{grydeland_comparison_2014,
  title = {Comparison of Three Generations of {{ActiGraph}} Activity Monitors under Free-Living Conditions: Do They Provide Comparable Assessments of Overall Physical Activity in 9-Year Old Children?},
  shorttitle = {Comparison of Three Generations of {{ActiGraph}} Activity Monitors under Free-Living Conditions},
  author = {Grydeland, May and Hansen, Bjoerge Herman and {Ried-Larsen}, Mathias and Kolle, Elin and Anderssen, Sigmund Alfred},
