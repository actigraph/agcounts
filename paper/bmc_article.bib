  month = dec,
  journal = {Sci Data},
  volume = {7},
  number = {1},
  pages = {219},
  issn = {2052-4463},
  doi = {10.1038/s41597-020-0563-y},
  abstract = {Abstract             Gait analysis has traditionally relied on laborious and lab-based methods. Data from wearable sensors, such as Inertial Measurement Units (IMU), can be analyzed with machine learning to perform gait analysis in real-world environments. This database provides data from thirty participants (fifteen males and fifteen females, 23.5\,{$\pm$}\,4.2 years, 169.3\,{$\pm$}\,21.5\,cm, 70.9\,{$\pm$}\,13.9\,kg) who wore six IMUs while walking on nine outdoor surfaces with self-selected speed (16.4\,{$\pm$}\,4.2\,seconds per trial). This is the first publicly available database focused on capturing gait patterns of typical real-world environments, such as grade (up-, down-, and cross-slopes), regularity (paved, uneven stone, grass), and stair negotiation (up and down). As such, the database contains data with only subtle differences between conditions, allowing for the development of robust analysis techniques capable of detecting small, but significant changes in gait mechanics. With analysis code provided, we anticipate that this database will provide a foundation for research that explores machine learning applications for mobile sensing and real-time recognition of subtle gait adaptations.},
  langid = {english},
  file = {/Users/ali.neishabouri/Zotero/storage/QV2LRDZW/Luo et al. - 2020 - A database of human gait performance on irregular .pdf}
}

