@article{bezuidenhout_validity_2021,
  title = {Validity of {{Hip}} and {{Ankle Worn Actigraph Accelerometers}} for {{Measuring Steps}} as a {{Function}} of {{Gait Speed}} during {{Steady State Walking}} and {{Continuous Turning}}},
