@article{sundararajan_sleep_2021,
  title = {Sleep Classification from Wrist-Worn Accelerometer Data Using Random Forests},
  author = {Sundararajan, Kalaivani and Georgievska, Sonja and Te Lindert, Bart H W and Gehrman, Philip R and Ramautar, Jennifer and Mazzotti, Diego R and Sabia, S{\'e}verine and Weedon, Michael N and {van Someren}, Eus J W and Ridder, Lars and Wang, Jian and {van Hees}, Vincent T},
