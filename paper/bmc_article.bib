@incollection{magjarevic_automated_2009,
  title = {Automated Detection of Tonic Seizures Using 3-{{D}} Accelerometry},
  booktitle = {4th {{European Conference}} of the {{International Federation}} for {{Medical}} and {{Biological Engineering}}},
  author = {Nijsen, Tamara M. E. and Aarts, Ronald M. and Arends, Johan B. A. M. and Cluitmans, Pierre J. M.},
