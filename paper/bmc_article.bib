  author = {Czech, Matthew D. and Psaltos, Dimitrios and Zhang, Hao and Adamusiak, Tomasz and Calicchio, Monica and Kelekar, Amey and Messere, Andrew and Van Dijk, Koene R. A. and Ramos, Vesper and Demanuele, Charmaine and Cai, Xuemei and Santamaria, Mar and Patel, Shyamal and Karahanoglu, F. Isik},
