}

@article{strain_wearable-device-measured_2020,
  title = {Wearable-Device-Measured Physical Activity and Future Health Risk},
  author = {Strain, Tessa and Wijndaele, Katrien and Dempsey, Paddy C. and Sharp, Stephen J. and Pearce, Matthew and Jeon, Justin and Lindsay, Tim and Wareham, Nick and Brage, S{\o}ren},
