@article{whitaker_comparison_2018,
  title = {Comparison of {{Two Generations}} of {{ActiGraph Accelerometers}}: {{The CARDIA Study}}},
  shorttitle = {Comparison of {{Two Generations}} of {{ActiGraph Accelerometers}}},
  author = {Whitaker, Kara M. and Gabriel, Kelley Pettee and Jacobs, David R. and Sidney, Stephen and Sternfeld, Barbara},
  year = {2018},
