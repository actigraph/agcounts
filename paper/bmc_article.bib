}

@article{staudenmayer_methods_2015,
  title = {Methods to Estimate Aspects of Physical Activity and Sedentary Behavior from High-Frequency Wrist Accelerometer Measurements},
  author = {Staudenmayer, John and He, Shai and Hickey, Amanda and Sasaki, Jeffer and Freedson, Patty},
