@article{bennett_evaluation_2016,
  title = {Evaluation of Pedometry as a Patient-Centered Outcome in Patients Undergoing Hematopoietic Cell Transplant ({{HCT}}): A Comparison of Pedometry and Patient Reports of Symptoms, Health, and Quality of Life},
  shorttitle = {Evaluation of Pedometry as a Patient-Centered Outcome in Patients Undergoing Hematopoietic Cell Transplant ({{HCT}})},
  author = {Bennett, Antonia V. and Reeve, Bryce B. and Basch, Ethan M. and Mitchell, Sandra A. and Meeneghan, Mathew and Battaglini, Claudio L. and {Smith-Ryan}, Abbie E. and Phillips, Brett and Shea, Thomas C. and Wood, William A.},
  year = {2016},
  month = mar,
  journal = {Qual Life Res},
  volume = {25},
  number = {3},
  pages = {535--546},
  issn = {1573-2649},
  doi = {10.1007/s11136-015-1179-0},
  abstract = {AIMS: We evaluated pedometry as a novel patient-centered outcome because it enables passive continuous assessment of activity and may provide information about the consequences of symptomatic toxicity complementary to self-report. METHODS: Adult patients undergoing hematopoietic cell transplant (HCT) wore pedometers and completed PRO assessments during transplant hospitalization (4 weeks) and 4 weeks post-discharge. Patient reports of symptomatic treatment toxicities (single items from PRO-CTCAE, http://healthcaredelivery.cancer.gov/pro-ctcae ) and symptoms, physical health, mental health, and quality of life (PROMIS(\textregistered ) Global-10, http://nih.promis.org ), assessed weekly with 7-day recall on Likert scales, were compared individually with pedometry data, summarized as average daily steps per week, using linear mixed models. RESULTS: Thirty-two patients [mean age 55 (SD = 14), 63 \% male, 84 \% white, 56 \% autologous, 43 \% allogeneic] completed a mean 4.6 (SD = 1.5, range 1-8) evaluable assessments. Regression model coefficients ({$\beta$}) indicated within-person decrements in average daily steps were associated with increases in pain ({$\beta$} = -852; 852 fewer steps per unit increase in pain score, p {$<$} 0.001), fatigue ({$\beta$} = -886, p {$<$} 0.001), vomiting ({$\beta$} = -518, p {$<$} 0.01), shaking/chills ({$\beta$} = -587, p {$<$} 0.01), diarrhea ({$\beta$} = -719, p {$<$} 0.001), shortness of breath ({$\beta$} = -1018, p {$<$} 0.05), reduction in carrying out social activities ({$\beta$} = 705, p {$<$} 0.01) or physical activities ({$\beta$} = 618, p {$<$} 0.01), and global physical health ({$\beta$} = 101, p {$<$} 0.001), but not global mental health or quality of life. CONCLUSIONS: In this small sample of HCT recipients, more severe symptoms, impaired physical health, and restrictions in the performance of usual daily activities were associated with statistically significant decrements in objectively measured daily steps. Pedometry may be a valuable outcome measure and validation anchor in clinical research.},
  langid = {english},
  pmid = {26577763},
  keywords = {Accelerometry,Activities of Daily Living,Adolescent,Adult,Aged,Cross-Sectional Studies,Female,Fitbit,Follow-Up Studies,Health Status Indicators,Hematopoietic cell transplant,Hematopoietic Stem Cell Transplantation,Humans,Male,Middle Aged,Motor Activity,Oncology,Patient Outcome Assessment,Pedometry,PRO-CTCAE,PROMIS Global-10,Quality of Life,Self Report,Validation,Young Adult},
  file = {/Users/ali.neishabouri/Zotero/storage/GW5GMCNG/Bennett et al. - 2016 - Evaluation of pedometry as a patient-centered outc.pdf}
}

@inproceedings{bertschi_accurate_2015,
