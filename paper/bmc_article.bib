@article{lee_step_2015,
  title = {Step {{Detection Robust}} against the {{Dynamics}} of {{Smartphones}}},
  author = {Lee, Hwan-hee and Choi, Suji and Lee, Myeong-jin},
  year = {2015},
  month = oct,
  journal = {Sensors},
  volume = {15},
  number = {10},
  pages = {27230--27250},
  issn = {1424-8220},
  doi = {10/gnh2s4},
  abstract = {A novel algorithm is proposed for robust step detection irrespective of step mode and device pose in smartphone usage environments. The dynamics of smartphones are decoupled into a peak-valley relationship with adaptive magnitude and temporal thresholds. For extracted peaks and valleys in the magnitude of acceleration, a step is defined as consisting of a peak and its adjacent valley. Adaptive magnitude thresholds consisting of step average and step deviation are applied to suppress pseudo peaks or valleys that mostly occur during the transition among step modes or device poses. Adaptive temporal thresholds are applied to time intervals between peaks or valleys to consider the time-varying pace of human walking or running for the correct selection of peaks or valleys. From the experimental results, it can be seen that the proposed step detection algorithm shows more than 98.6\% average accuracy for any combination of step mode and device pose and outperforms state-of-the-art algorithms.},
  langid = {english},
  keywords = {accelerometer,adaptive magnitude threshold,adaptive temporal threshold,device pose,peak-valley relationship,step average,step detection,step mode},
  file = {/Users/ali.neishabouri/Zotero/storage/MCSTHAC4/Lee et al. - 2015 - Step Detection Robust against the Dynamics of Smar.pdf;/Users/ali.neishabouri/Zotero/storage/MIP8ZG4E/Lee et al. - 2015 - Step Detection Robust against the Dynamics of Smar.pdf}
}

